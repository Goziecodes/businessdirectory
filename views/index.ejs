            </div>
            <div class="container">
                <div class="row justify-content-center pt-5">
                 
                </div>
            </div>
        </section>
        <!--End Page Inner -->

        <!-- Categories --> <section class="lis-grediant grediant-tb-white">
        <div class="container">
         <div class="row justify-content-center">
        <div class="col-12 col-md-10 text-center"> 
            <div class="heading pb-4">
        <h2 class="f-weight-500">Find the Best Places</h2>   
          </div> 
         </div> 
     </div>

        <div class="row">  
           <div class="col-4 col-sm-2 col-lg-2 wow fadeInUp mb-4 mb-lg-0 text-center">
            <a href="/business/hotel" class="text-white">
        <div class="lis-categories lis-bg1 rounded lis-font-poppins "> 
            <div class="text-white mb-2 h2">
                <i class="icofont icofont-hotel-alt"></i>
            </div> Hotel
        
     </div> 
      </a>
        </div>
                    
                   
                    <div class="col-4 col-sm-2 col-lg-2 wow fadeInUp mb-4 mb-sm-0 text-center">
                        <a href="/business/eatries" class="text-white">
                            <div class="lis-categories lis-bg4 rounded lis-font-poppins ">
                                <div class="text-white mb-2 h2">
                                    <i class="icofont icofont-fast-food"></i>
                                </div>
                                Eatries 
                               
                            </div> 
                        </a>
                    </div>
                
                    <div class="col-4 col-sm-2 col-lg-2 wow fadeInUp text-center">
                        <a href="/business/medical" class="text-white">
                            <div class="lis-categories lis-bg6 rounded lis-font-poppins ">
                                <div class="text-white mb-2 h2"><i class="icofont icofont-medical-sign-alt"></i></div>
                                Medical
                                
                            </div> 
                        </a>
                    </div>
                   
                    <div class="col-4 col-sm-2 col-lg-2 wow fadeInUp text-center">
                        <a href="/business/school" class="text-white">
                            <div class="lis-categories lis-bg2 rounded lis-font-poppins ">
                                <div class="text-white mb-2 h2"><i class="icofont icofont-medical-sign-alt"></i></div>
                                Schools
                                
                            </div> 
                        </a>
                    </div>
                  
                    <div class="col-4 col-sm-2 col-lg-2 wow fadeInUp text-center">
                        <a href="/business/mall" class="text-white">
